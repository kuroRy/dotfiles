{
  "permissions": {
    "allow": [
      "Bash(npm test)",
      "List(*)",
      "Fetch(https://*)",
      "Edit(*)",
      "Bash(git:*)",
      "Bash(ls:*)",
      "Bash(cat:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(cp:*)",
      "Bash(make test:*)",
      "Bash(make:*)",
      "Bash(tree:*)",
      "Bash(find:*)",
      "Bash(rg:*)",
      "Bash(grep:*)",
      "Bash(chmod:*)",
      "Bash(docker compose:*)"
    ],
    "deny": [
      "Bash(rm:*)",
      "Bash(sudo:*)",
      "Read(.env.*)"
    ],
    "defaultMode": "acceptEdits"
  },
  "model": "opus",
  "language": "日本語"
}
